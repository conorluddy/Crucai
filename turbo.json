{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "check-types": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": []
    },
    "lint": {},
    "prettier": {},
    "test": {
      "outputs": []
    },
    "patch": {
      "outputs": []
    },
    "publish": {
      "dependsOn": ["build", "test"],
      "outputs": []
    }
  }
}
